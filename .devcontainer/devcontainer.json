{
    "image": "ubuntu:22.04",
    "waitFor": "onCreateCommand",
    "onCreateCommand": "bash .devcontainer/init-env.sh",
    "updateContentCommand": "cspkg -i ecs_bootstrap netutils.csym --yes",
    "postAttachCommand": {
        "server": "ecs hello.ecs"
    },
    "portsAttributes": {
        "5000": {
            "label": "Application",
            "onAutoForward": "openPreview"
        }
    },
    "customizations": {
        "codespaces": {
            "openFiles": [
                "hello.ecs"
            ]
        },
        "vscode": {
            "extensions": [
                "mikecovlee.covscript"
            ]
        }
    },
    "forwardPorts": [
        5000
    ]
}